		{
			"name": "[concat(parameters('factoryName'), '/Moving Average1')]",
			"type": "Microsoft.DataFactory/factories/dataflows",
			"apiVersion": "2018-06-01",
			"properties": {
				"type": "Flowlet",
				"typeProperties": {
					"sources": [],
					"sinks": [],
					"transformations": [
						{
							"name": "MovingAvgWindow"
						},
						{
							"name": "DerivedColumn1"
						},
						{
							"name": "StocksSource"
						},
						{
							"name": "output1"
						}
					],
					"scriptLines": [
						"input(output(",
						"          Value as double,",
						"          CommonLabel as string",
						"     ),",
						"     order: 0) ~> StocksSource",
						"DerivedColumn1 window(over(CommonLabel),",
						"     asc(MyDate, true),",
						"     startRowOffset: -7L,",
						"     endRowOffset: 7L,",
						"     MovingAvg = round(avg(Value),2)) ~> MovingAvgWindow",
						"StocksSource derive(MyDate = toDate('10/10/1998')) ~> DerivedColumn1",
						"MovingAvgWindow output(mapColumn(",
						"          Value,",
						"          CommonLabel,",
						"          Date = MyDate,",
						"          MovingAvg",
						"     )) ~> output1"
					]
				}
			},
			"dependsOn": []
		},
